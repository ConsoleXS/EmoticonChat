{
  "schemaVersion": 1,
  "id": "trishieldmod",
  "version": "${version}",

  "name": "EmoticonChat",
  "description": "A fabric mod that allows you to put emojis in chat",
  "authors": [
    "ConsoleX"
  ],
  "contact": {
    "homepage": "https://fabricmc.net/",
    "sources": "https://github.com/FabricMC/fabric-example-mod"
  },

  "license": "MIT",
  "icon": "assets/modid/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "com.consolex.emoticonchat.EmoticonChatMod"
    ]
  },
  "mixins": [
    "emoticon.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.14.6",
    "fabric": "*",
    "minecraft": "~1.18.2",
    "java": ">=17"
  },
  "suggests": {
    "another-mod": "*"
  }
}
